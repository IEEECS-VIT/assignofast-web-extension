{
    "manifest_version": 3,
    "name": "VTOP Digital Assignment Scraper",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["https://vtop.vit.ac.in/*"],
            "js": ["content.js"]
        }
    ],
    "host_permissions": [
        "https://vtop.vit.ac.in/*",
        "https://assignofast-backend.vercel.app/*"
    ]
}