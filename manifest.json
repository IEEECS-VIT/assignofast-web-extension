{
    "manifest_version": 3,
    "name": "Assignofast",
    "version": "1.1.1",
    "description": "Your personal shield against deadline dread - Assignofast keeps you always ahead",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },  
    "permissions": [
        "activeTab",
        "scripting",
        "storage",
        "identity",
        "identity.email"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://vtop.vit.ac.in/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "host_permissions": [
        "https://vtop.vit.ac.in/*",
        "https://assignofast-backend.vercel.app/*"
    ],
    "oauth2": {
        "client_id": "889572280066-en3he9vvnikldcrtigsfdfe5h1jsmj2e.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/userinfo.email",
            "https://www.googleapis.com/auth/userinfo.profile",
            "openid"
        ]
    }
}